@model IEnumerable<WebApi.DataAccess.UserDb>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div class="container">
    <h2>Contextual Classes</h2>
    <p>Contextual classes can be used to color table rows or table cells. The classes that can be used are: .active, .success, .info, .warning, and .danger.</p>
    <table class="table">
        <thead>
            <tr>
                <th>Firstname</th>
                <th>Lastname</th>
                <th>Email</th>
                <th>Password</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var j in Model)
            {
            <tr>
                <td class="success">
                    @j.FirstName
                </td>
                <td class="success">
                    @j.LastName
                </td>
                <td class="success">
                    @j.Email
                </td>

                <td class="success">
                    @j.Password
                </td>
            </tr>
                    }
                

            </tbody>
    </table>
</div>

